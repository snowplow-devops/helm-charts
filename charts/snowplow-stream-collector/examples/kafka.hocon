collector {
  interface = "0.0.0.0"
  port = ${COLLECTOR_PORT}

  streams {
    good = "good"
    bad = "bad"

    # Assumes you have a locally deployed Kafka cluster
    # e.g. helm upgrade --install kafka bitnami/kafka
    sink {
      brokers = "kafka-0.kafka-headless.default.svc.cluster.local:9092"
    }
  }
}
